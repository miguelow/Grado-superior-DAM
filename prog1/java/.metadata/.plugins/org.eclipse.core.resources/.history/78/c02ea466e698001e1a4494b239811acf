package ejecutables;

public class OperacionesMatematicas {
	
	public static void main (String[] args) {
		/*
		* Operaciones matematicas
		*/
		
		int num = 6; 
		int resultado = 0;
		
		resultado = num++;
		
		System.out.println("num : " + num);
		System.out.println("resultado : " + resultado);
	}
}
